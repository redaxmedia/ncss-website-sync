{
	"name": "redaxmedia/ncss-website-sync",
	"description": "Yet another cronjob for NCSS's website",
	"version": "4.1.0",
	"homepage": "https://ncss.io",
	"license": "GPL-3.0",
	"repositories":
	[
		{
			"type": "git",
			"url": "https://github.com/redaxmedia/ncss-template.git"
		}
	],
	"require":
	{
		"php": "^7.2"
	},
	"require-dev":
	{
		"redaxmedia/ncss-template": "dev-master",
		"redaxscript/redaxscript": "dev-master"
	},
	"scripts":
	{
		"post-install-cmd":
		[
			"cd vendor/redaxscript/redaxscript && composer install"
		],
		"post-update-cmd":
		[
			"@post-install-cmd"
		]
	}
}
